{
  "name": "qdrant-sink-connector",
  "config": {
    "connector.class": "io.qdrant.kafka.QdrantSinkConnector",
    "topics.regex": "ProjectManagement\\..*",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "qdrant.grpc.url": "http://qdrant:6334",
    "qdrant.collection.name": "pmscollection",
    "qdrant.vectors.field": "vector",
    "qdrant.payload.fields": "id,text,payload,source_collection",
    "qdrant.id.field": "id",
    "errors.deadletterqueue.topic.name": "dead_queue",
    "errors.deadletterqueue.topic.replication.factor": "1"
  }
}